<launch>
  <node pkg="rover_driver" exec="base" namespace="rover">
    <!-- <param name="vid" type="str" value="0483"/> -->
    <!-- <param name="pid" type="str" value="5740"/> -->
  </node>
   <node pkg="orb_slam3" exec="mono_inertial" namespace="slam" output='screen' emulate_tty="True">
    <param name="voc_file" value="assets/vocabulary.bin"/>
    <param name="settings_file" value="assets/RoverMaster.yaml"/>
    <remap from="imu/get" to="/rover/base/imu/get"/>
    <remap from="imu/get" to="/rover/camera/img/get"/>
    <remap from="img/get" to="/camera_0/img"/>
  </node>
  <node pkg="spinnaker_camera" exec="spinnaker_camera"/>
  <node pkg="socket_agent" exec="agent">
    <remap from="vel/get" to="/rover/base/velocity/get"/>
    <remap from="vel/set" to="/rover/base/velocity/set"/>
    <remap from="imu/acc" to="/rover/base/imu/acc"/>
    <remap from="imu/att" to="/rover/base/imu/att"/>
    <remap from="imu/get" to="/rover/base/imu/get"/>
  </node>
  <!-- Visualization -->
  <!-- <node pkg="rviz2" exec="rviz2" name="rviz2" output="screen">
    <param name="use_sim_time" value="true"/>
    <param name="fixed_frame" value="map"/>
    <param name="target_frame" value="map"/>
  </node> -->
  <!-- Image View -->
  <!-- <node pkg="rqt_image_view" exec="rqt_image_view"/> -->
</launch>

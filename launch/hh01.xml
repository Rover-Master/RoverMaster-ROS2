<launch>
  <!-- The "face detector" node -->
  <node pkg="hh01" exec="main" name="hh01_main" namespace="hh01"/>
  <!-- Camera Capturing Node -->
  <node pkg="usb_cam" exec="usb_cam_node_exe" name="usb_cam">
    <remap from="/image_raw" to="/hh01/img/in"/>
  </node>
  <!-- GUI Image Viewer -->
  <node pkg="rqt_image_view" exec="rqt_image_view" name="image_view"/>
</launch>